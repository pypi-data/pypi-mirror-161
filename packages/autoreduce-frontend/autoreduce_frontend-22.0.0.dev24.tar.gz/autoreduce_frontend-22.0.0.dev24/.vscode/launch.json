    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run webapp",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/autoreduce_frontend/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "presentation": {
                "hidden": false,
                "group": "autoreduce-run",
                "order": 1
            },
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "justMyCode": false
        },
    ]
}
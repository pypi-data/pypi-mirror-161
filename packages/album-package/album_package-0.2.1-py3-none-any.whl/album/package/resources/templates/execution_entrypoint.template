import call_solution

answer = input("Should the solution be launched with a GUI? (y/n)")
while (True):
    if answer == 'y' or answer == 'yes':
        call_solution.main(True)
        break
    elif answer == 'n' or answer == 'no':
        call_solution.main(False)
        break
    else:
        answer = input("Invalid choice please enter y/yes or n/no.")
    "spark": "3.2",
    "v": "1.1.0"
  },
  "link": "https://github.com/JetBrains/kotlin-spark-api",
  "dependencies": [
    "org.jetbrains.kotlinx.spark:kotlin-spark-api-jupyter-$spark:$v"
  ],
  "integrationTypeNameRules" : [
    "-:org.jetbrains.kotlinx.spark.api.jupyter.**",
    "+:org.jetbrains.kotlinx.spark.api.jupyter.SparkStreamingIntegration"
  ]
}